struct Vec {
    x: int,
    y: int,

    def __add__(self, other: Vec) -> Vec {
        return Vec(self.x + other.x, self.y + other.y)
    }

    def __add__(self, other: int) -> Vec {
        return Vec(self.x + other, self.y + other)
    }

    def __str__(self) -> str {
        return  "Vec(" + str(self.x) + ", " + str(self.y) + ")"
    }
}

a: Vec = Vec(2, 5)
print(str(a.x))
print(str(a.x + 1))
print("a: " + str(a))
print(str(a.x))
print(str(a.x + 1))